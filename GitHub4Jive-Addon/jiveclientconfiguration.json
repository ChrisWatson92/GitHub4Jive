{
    "clientUrl": "REPLACE_ME",
    "port": "8090",
    "development": true,
    "logLevel": "DEBUG",
    "ignoreExtensionRegistrationSource" : true,
    "extensionInfo" : {
        "name" : "GitHub4Jive",
        "description" : "Connect your GitHub repo to Jive to collaborate on Issues, Commits and Pull Requests and manage your repo."
    },
    "jive" : {
        "oauth2": {
            "clientOAuth2CallbackUrl": "/jive/oauth/callback"
        }
    },
    "github" : {
        "oauth2" : {
            "clientOAuth2CallbackUrl" : "/github/oauth/callback",
            "oauth2ConsumerKey": "REPLACE_ME",
            "oauth2ConsumerSecret": "REPLACE_ME",
            "oauth2Scope": "public_repo,user",
            "originServerAuthorizationUrl": "https://github.com/login/oauth/authorize",
            "originServerTokenRequestUrl": "https://github.com/login/oauth/access_token"
        },
        "webHookUrl" :  "/github/WebHookPortal"
    }
}